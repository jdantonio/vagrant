%% @version 0.1

-module(${1:`vim_snippets#Filename('', 'my')`}).

-behaviour(application).

-export([start/2, stop/1]).

start(_Type, _StartArgs) ->
	case ${2:root_supervisor}:start_link() of
		{ok, Pid} ->
			{ok, Pid};
	  Other ->
		  {error, Other}
	  end.

stop(_State) ->
	ok.
